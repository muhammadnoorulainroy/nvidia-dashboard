# =============================================================================
# NVIDIA DASHBOARD FRONTEND - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in the values
# Variables must be prefixed with VITE_ to be exposed to the frontend
# =============================================================================

# =============================================================================
# API Configuration
# =============================================================================
# Backend API URL (for proxy in development)
VITE_API_URL=http://localhost:8001

# API path prefix (relative path used by axios)
VITE_API_PREFIX=/api

# =============================================================================
# Server Configuration
# =============================================================================
# Development server port
VITE_PORT=3001

# Preview server port (for production build preview)
VITE_PREVIEW_PORT=3001

# =============================================================================
# Application Settings
# =============================================================================
# Application title
VITE_APP_TITLE=Nvidia Dashboard

# Debug mode (enables source maps in production build)
VITE_DEBUG=false
