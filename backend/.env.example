# =============================================================================
# Nvidia Dashboard - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in the values.
# NEVER commit .env to version control!
# =============================================================================

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================
DEBUG=false
# HOST=0.0.0.0
# PORT=8001

# =============================================================================
# POSTGRESQL DATABASE (REQUIRED)
# =============================================================================
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=nvidia

# =============================================================================
# GOOGLE CLOUD / BIGQUERY (REQUIRED)
# =============================================================================
GCP_PROJECT_ID=turing-gpt
BIGQUERY_DATASET=prod_labeling_tool_n
PROJECT_ID_FILTER=36

# Path to Google Cloud service account JSON file (optional if using ADC)
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# =============================================================================
# CORS SETTINGS
# =============================================================================
CORS_ORIGINS=http://localhost:3001,http://localhost:3000

# =============================================================================
# RATE LIMITING
# =============================================================================
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=minute
RATE_LIMIT_SYNC_REQUESTS=5
RATE_LIMIT_SYNC_WINDOW=minute

# =============================================================================
# DATA SYNC SETTINGS
# =============================================================================
SYNC_INTERVAL_HOURS=1
INITIAL_SYNC_ON_STARTUP=true

# =============================================================================
# PROJECT SETTINGS
# =============================================================================
PROJECT_START_DATE=2025-09-26

# =============================================================================
# JIBBLE INTEGRATION (OPTIONAL)
# =============================================================================
# JIBBLE_API_KEY=your_jibble_client_id
# JIBBLE_API_SECRET=your_jibble_client_secret
# JIBBLE_PROJECT_NAME=Nvidia - SysBench
